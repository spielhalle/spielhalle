{
    "name": "@spielhalle/sudoku-board",
    "version": "0.5.1",
    "description": "A tank call library",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "private": false,
    "scripts": {
        "build": "rollup -c ./rollup.config.js && npm run build:types && npm run build:webpack",
        "build:webpack": "webpack --mode=production",
        "build:types": "tsc --project ./tsconfig.json  -d --declarationDir dist/types --declarationMap --emitDeclarationOnly",
        "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
        "serve": "webpack serve",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src",
        "postlint": "prettier --config ./../../.prettierrc src/**/*.ts --check",
        "lint:fix": "eslint -c .eslintrc.js --ext .ts ./src --fix",
        "postlint:fix": "prettier --config ./../../.prettierrc src/**/*.ts --write",
        "docs": "typedoc --options ./typedoc.config.js",
        "prepublishOnly": "npm run test && npm run docs && npm run build",
        "version": "npm run build:readme && git add README.md"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://spielhalle.github.io/spielhalle/",
    "author": {
        "name": "donmahallem",
        "email": "donmahallem@users.noreply.github.com",
        "url": "https://github.com/donmahallem"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/spielhalle/spielhalle/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spielhalle/spielhalle.git"
    },
    "devDependencies": {
        "brfs": "2.0.2",
        "copy-webpack-plugin": "11.0.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.5.0",
        "source-map-loader": "3.0.1",
        "transform-loader": "0.2.4",
        "ts-loader": "9.3.0",
        "typescript": "4.6.4",
        "webpack": "5.73.0",
        "webpack-bundle-analyzer": "4.5.0",
        "webpack-cli": "4.9.2",
        "webpack-dev-server": "4.9.1"
    },
    "dependencies": {
        "@pixi/app": "~6.3.2",
        "@pixi/constants": "~6.3.2",
        "@pixi/core": "~6.3.2",
        "@pixi/display": "~6.3.2",
        "@pixi/graphics": "~6.3.2",
        "@pixi/interaction": "~6.3.2",
        "@pixi/math": "~6.3.2",
        "@pixi/runner": "~6.3.2",
        "@pixi/settings": "~6.3.2",
        "@pixi/sprite": "~6.3.2",
        "@pixi/text": "~6.3.2",
        "@pixi/ticker": "~6.3.2",
        "@pixi/utils": "~6.3.2"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=14.0.0"
    }
}
