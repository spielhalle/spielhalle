{
  "name": "@spielhalle/client",
  "version": "0.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "docs": "npx compodoc -p src/tsconfig.app.json -d ./docs",
    "postinstall": "npm run build:pug",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "build:githubpages": "ng build --aot --prod --configuration=production,githubpages",
    "prerender": "ng run spielhalle:prerender",
    "prerender:githubpages": "npx ng run spielhalle:prerender --configuration=production,githubpages --no-guess-routes --routes / --routes /tank-call",
    "build:pug": "pug ./src --doctype html",
    "build:ssr": "ng build && ng run spielhalle:server",
    "dev:ssr": "ng run spielhalle:serve-ssr",
    "bundle-analyze": "webpack-bundle-analyzer ./dist/browser/stats.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://spielhalle.github.io/spielhalle/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spielhalle/spielhalle/issues"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spielhalle/spielhalle.git"
  },
  "dependencies": {
    "@angular/animations": "13.3.4",
    "@angular/cdk": "13.3.4",
    "@angular/common": "13.3.4",
    "@angular/compiler": "13.3.4",
    "@angular/core": "13.3.4",
    "@angular/forms": "13.3.4",
    "@angular/material": "13.3.4",
    "@angular/platform-browser": "13.3.4",
    "@angular/platform-browser-dynamic": "13.3.4",
    "@angular/platform-server": "13.3.4",
    "@angular/router": "13.3.4",
    "@angular/service-worker": "13.3.4",
    "@donmahallem/sudoku": "~0.3.3",
    "@nguniversal/express-engine": "13.1.0",
    "@pixi/app": "~6.3.0",
    "@spielhalle/sudoku-board": "^0.5.1",
    "@spielhalle/tank-call": "^0.5.1",
    "async-mutex": "^0.3.2",
    "core-js": "3.22.2",
    "express": "4.18.0",
    "material-design-icons": "3.0.1",
    "rxjs": "~7.5.5",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.1",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.4",
    "@angular/language-service": "13.3.4",
    "@compodoc/compodoc": "1.1.19",
    "@nguniversal/builders": "13.1.0",
    "@types/express": "4.17.13",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.29",
    "apply-loader": "2.0.0",
    "codelyzer": "6.0.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "jasmine-core": "4.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "pug": "3.0.2",
    "pug-cli": "1.0.0-alpha6",
    "pug-loader": "2.4.0",
    "ts-loader": "9.2.9",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "tslint": "6.1.3",
    "typescript": "4.6.3",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  }
