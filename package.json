{
  "name": "root",
  "private": true,
  "devDependencies": {
    "@donmahallem/eslint-config": "1.0.57",
    "@donmahallem/rollup-config": "2.0.12",
    "@donmahallem/tslint-config": "0.3.2",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "8.3.1",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.26",
    "@types/sinon": "10.0.11",
    "chai": "4.3.6",
    "commitizen": "4.2.4",
    "conventional-changelog-angular": "5.0.13",
    "coveralls": "3.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "fs-extra": "10.0.1",
    "lerna": "4.0.0",
    "mocha": "9.2.2",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "rollup": "2.70.1",
    "sinon": "13.0.1",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typedoc": "0.22.15",
    "typescript": "4.6.3"
  },
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "coverage:merge": "node ./scripts/merge_coverage.js",
    "lint": "lerna run lint",
    "docs": "lerna run docs",
    "lint:fix": "lerna run lint:fix",
    "version-temp": "npx conventional-changelog --preset eslint --release-count 0 --outfile ./CHANGELOG.md --verbose && npx lerna exec --concurrency 1 --stream -- 'conventional-changelog --preset eslint --release-count 0 --pkg ./package.json --outfile ./CHANGELOG.md --verbose'"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  }
}
